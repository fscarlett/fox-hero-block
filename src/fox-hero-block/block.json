{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/fox-hero-block",
	"version": "0.1.0",
	"title": "Fox Hero Block",
	"category": "widgets",
	"icon": "superhero",
	"description": "Gutenberg block for simple hero section.",
	"example": {},
	"supports": {
		"html": false
	},

	"attributes": {
		"heroImgId": {
			"type": "number",
			"default": "0"
		},
		"pickedHeroImgUrl": {
			"type": "string",
			"default": ""
		},
		"mobImgId": {
			"type": "number",
			"default": "0"
		},
		"pickedMobImgUrl": {
			"type": "string",
			"default": ""
		},
		"heroImgSourceUrl": {
			"type": "string",
			"default": ""
		},
		"heroImgMobSourceUrl": {
			"type": "string",
			"default": ""
		},
		"imgVertPosition": {
			"type": "string",
			"default": "0"
		},
		"heroTitle": {
			"type": "string",
			"default": ""
		},
		"heroParagraph": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "fox-hero-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
